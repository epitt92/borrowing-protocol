{
  "name": "borrowing-protocol",
  "version": "0.0.1",
  "description": "bonq smart contracts for self sovereign, over collateralized borrowing",
  "main": "index.js",
  "scripts": {
    "test": "hardhat test",
    "test-trace": "hardhat test --trace",
    "test-fulltrace": "hardhat test --fulltrace",
    "test:subgraph": "graph test",
    "compile": "hardhat compile",
    "cuke": "hardhat compile && tsc && cucumber-js",
    "ganache": "ganache-cli --port 7545 --account=\"0xd9066ff9f753a1898709b568119055660a77d9aae4d7a4ad677b8fb3d2a571e5, 1000000000000000000000\" --account=\"0xa2f63ae34aa974a978e2b93e9f96e810818650c8bc60757f921d62e3ce37646a, 1000000000000000000000\" --account=\"0x5de4111afa1a4b94908f83103eb1f1706367c2e68ca870fc3fb9a804cdab365a, 1000000000000000000000\" --account=\"0x7c852118294e51e653712a81e05800f419141751be58f605c371e15141b007a6, 1000000000000000000000\" --account=\"0x47e179ec197488593b187f80a00eb0da91f1b9d0b13f8733639f19c30a34926a, 1000000000000000000000\" --account=\"0x8b3a350cf5c34c9194ca85829a2df0ec3153be0318b5e2d3348e872092edffba, 1000000000000000000000\" --account=\"0x92db14e403b83dfe3df233f83dfa3a0d7096f21ca9b0d6d6b8d88b2b4ec1564e, 1000000000000000000000\" --account=\"0x4bbbf85ce3377467afe5d46f804f221813b2bb87f24d81f60f1fcdbf7cbf4356, 1000000000000000000000\" --account=\"0xdbda1821b80551c9d65939329250298aa3472ba22feea921c0cf5d620ea67b97, 1000000000000000000000\" --account=\"0x2a871d0798f97d79848a013d4936a73bf4cc922c825d33c1cf7073dff6d409c6, 1000000000000000000000\"",
    "prepare:manifest": "node scripts/helpers/prepare-subgraph-manifest-for-deploy.js --network $NETWORK",
    "deploy": "echo $NETWORK && rm -rf ./deployments_package/deployments/$NETWORK/ && hardhat run scripts/deploy-bonq-protocol.ts --network $NETWORK && cp -R ./src/types ./deployments_package/ && cp -R ./src/types ./deployments_package/deployments/$NETWORK/ && cp -R ./artifacts ./deployments_package/deployments/$NETWORK/",
    "sourcify": "for f in deployments_package/deployments/$NETWORK/*.json; do npx hardhat sourcify --network $NETWORK --contract-name  $(basename \"$f\" .json); done",
    "slither": " slither . --exclude-dependencies --exclude-low --exclude-informational --hardhat-ignore-compile --filter-paths \"contracts/test,contracts/interfaces/uniswap,node_modules\" --json slither-result.json",
    "pf-test:merge:volta": "hardhat run --network volta scripts/price-feed-merge-into-dev-tests.js",
    "external-pf-test:merge:volta": "hardhat run --network volta scripts/external-price-feed-merge-into-dev-tests.js",
    "graph-test:merge:volta-test": "hardhat run --network volta-test scripts/subgraph-merge-into-dev-test.js",
    "graph-test:merge:local": "hardhat run --network localhost scripts/subgraph-merge-into-dev-test.js",
    "graph:codegen": "rm -rf generated && npx graph codegen",
    "graph:deploy:chain-stack": "node scripts/helpers/prepare-subgraph-manifest-for-deploy.js --network matic && graph deploy --node https://api.graph-eu.p2pify.com/3ad4475c08e20b6bae66821389e74ae1/deploy --ipfs https://api.graph-eu.p2pify.com/3ad4475c08e20b6bae66821389e74ae1/ipfs borrowing-protocol",
    "graph:create:dev": "npx graph create borrowing-protocol --node http://graphprotocol-node-query.graphnode:8020",
    "graph:deploy:dev": "echo v.0.0.1 | npx graph deploy borrowing-protocol --ipfs http://ipfs-ipfs.ipfs:5001 --node http://graphprotocol-node-query.graphnode:8020",
    "graph:deploy:dev:version": "npx graph deploy borrowing-protocol --ipfs http://ipfs-ipfs.ipfs:5001 --node http://graphprotocol-node-query.graphnode:8020",
    "graph:create:dev:test": "npx graph create borrowing-protocol-test --node http://graphprotocol-node-query.graphnode:8020",
    "graph:deploy:dev:test": "echo v.0.0.1 | npx graph deploy borrowing-protocol-test --ipfs http://ipfs-ipfs.ipfs:5001 --node http://graphprotocol-node-query.graphnode:8020",
    "graph:remove:dev:test": "npx graph remove borrowing-protocol-test --node http://graphprotocol-node-query.graphnode:8020",
    "graph:create:dev:test-temp": "npx graph create borrowing-protocol-test-temp --node http://graphprotocol-node-query.graphnode:8020",
    "graph:deploy:dev:test-temp": "echo v.0.0.1 | npx graph deploy borrowing-protocol-test-temp --ipfs http://ipfs-ipfs.ipfs:5001 --node http://graphprotocol-node-query.graphnode:8020",
    "graph:remove:dev:test-temp": "npx graph remove borrowing-protocol-test-temp --node http://graphprotocol-node-query.graphnode:8020",
    "graph:create:docker": "npx graph create borrowing-protocol --node http://ganache:8020",
    "graph:deploy:docker": "echo v.0.0.1 | npx graph deploy borrowing-protocol --ipfs http://ipfs:5001 --node http://ganache:8020",
    "graph:remove:docker": "npx graph remove borrowing-protocol --node http://ganache:8020",
    "graph:create:local": "npx graph create borrowing-protocol --node http://localhost:8020",
    "graph:deploy:local": "echo v.0.0.4 | npx graph deploy borrowing-protocol --ipfs http://localhost:5001 --node http://localhost:8020",
    "graph:remove:local": "npx graph remove borrowing-protocol --node http://localhost:8020",
    "graph:create:ci": "npx graph create borrowing-protocol --node $GRAPH_NODE_ENDPOINT",
    "graph:deploy:ci": "echo $VERSION | npx graph deploy borrowing-protocol --ipfs $IPFS_ENDPOINT --node $GRAPH_NODE_ENDPOINT",
    "graph:deploy:ci:version": "npx graph deploy borrowing-protocol --ipfs $IPFS_ENDPOINT --node $GRAPH_NODE_ENDPOINT",
    "graph:create:ci:test": "npx graph create borrowing-protocol-test --node $GRAPH_NODE_ENDPOINT",
    "graph:deploy:ci:test": "echo $VERSION | npx graph deploy borrowing-protocol-test --ipfs $IPFS_ENDPOINT --node $GRAPH_NODE_ENDPOINT",
    "graph:remove:ci:test": "npx graph remove borrowing-protocol-test --node $GRAPH_NODE_ENDPOINT",
    "graph:create:ci:test-temp": "npx graph create borrowing-protocol-test-temp --node $GRAPH_NODE_ENDPOINT",
    "graph:deploy:ci:test-temp": "echo $VERSION | npx graph deploy borrowing-protocol-test-temp --ipfs $IPFS_ENDPOINT --node $GRAPH_NODE_ENDPOINT",
    "graph:remove:ci:test-temp": "npx graph remove borrowing-protocol-test-temp --node $GRAPH_NODE_ENDPOINT",
    "clean": "rm -rf src reports artifacts build cache",
    "solhint": "solhint -f stylish './contracts/*.sol' './contracts/interfaces/*.sol' './contracts/utils/*.sol'",
    "eslint:typescript": "eslint ./test --ext .ts",
    "eslint:typescript:fix": "eslint --fix ./test --ext .ts",
    "prettier": "npm run prettier:solidity && npm run prettier:tests:typescript",
    "prettier:solidity": "prettier --config .prettierrc.json 'contracts/**/*.sol' --write",
    "prettier:tests:typescript": "prettier --config .prettierrc.json 'test/**/*.ts' --write",
    "function-hashes": "grep -E 'modifier|function|constructor' contracts/*.sol contracts/utils/*.sol | grep -v '*\\|\\/\\/' | while read line; do echo $line | cut -d'{' -f 1; echo $line | openssl sha256 | cut -c 9-14; printf '&'; done  | tr '\\n' ' ' | tr '&' '\\n' > hashes.txt",
    "node": "npx hardhat node --hostname 0.0.0.0",
    "prepare": "husky install",
    "liquidations:volta": "npx hardhat run scripts/liquidate-troves-function.ts --network volta"
  },
  "author": "bonq AG",
  "license": "MIT",
